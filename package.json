{
  "name": "rc-texty-anim",
  "version": "0.1.0",
  "description": "texty-anim ui component for react",
  "keywords": [
    "react",
    "react-component",
    "react-texty-anim",
    "texty-anim"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "homepage": "https://github.com/react-component/texty-anim",
  "author": "155259966@qq.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/react-component/texty-anim.git"
  },
  "bugs": "https://github.com/react-component/texty-anim/issues",
  "files": [
    "lib",
    "assets/*.css",
    "dist",
    "es"
  ],
  "license": "MIT",
  "main": "./lib/index",
  "module": "./es/index",
  "config": {
    "port": 8010,
    "entry": {
      "rc-texty-anim": [
        "./assets/index.less",
        "./src/index.tsx"
      ]
    }
  },
  "scripts": {
    "dist": "rc-tools run dist",
    "build": "rc-tools run build",
    "gh-pages": "rc-tools run gh-pages",
    "start": "rc-tools run server",
    "compile": "rc-tools run compile --babel-runtime",
    "pub": "rc-tools run pub --babel-runtime",
    "lint": "rc-tools run lint --fix",
    "karma": "rc-test run karma",
    "saucelabs": "rc-test run saucelabs",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "babel-runtime": "6.x",
    "classnames": "^2.2.6",
    "rc-tween-one": "^2.1.0"
  },
  "devDependencies": {
    "core-js": "^2.5.1",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "enzyme-to-json": "^3.2.1",
    "expect.js": "0.3.x",
    "jest": "^23.1.0",
    "pre-commit": "1.x",
    "raf": "^3.4.0",
    "rc-tools": "^8.1.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "pre-commit": [
    "lint"
  ],
  "jest": {
    "setupFiles": [
      "./tests/setup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "\\.jsx?$": "./node_modules/rc-tools/scripts/jestPreprocessor.js"
    }
  }
}
